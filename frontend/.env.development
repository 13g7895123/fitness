# Port 設定
VITE_API_PORT=9103
VITE_FRONTEND_PORT=9202

# API Base URL
VITE_API_BASE_URL=http://localhost:9103/api/v1

# App URL
VITE_APP_URL=http://localhost:9202

# LINE Login
VITE_LINE_CHANNEL_ID=YOUR_CHANNEL_ID
VITE_LINE_CALLBACK_URL=http://localhost:9202/auth/callback

# 使用 Mock 登入 (開發環境使用)
VITE_USE_MOCK_AUTH=true

# 啟用測試登入按鈕
VITE_ENABLE_TEST_LOGIN=true

# 環境
VITE_ENV=development
